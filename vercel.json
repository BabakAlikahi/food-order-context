{
  "version": 2,
  "builds": [
    {
      "src": "frontend/index.html",
      "use": "@vercel/static",
      "routes": [
        {
          "src": "/api/.*",
          "dest": "backend/server.js"
        },
        {
          "src": "/(.*)",
          "dest": "frontend/$1"
        }
      ]
    }
  ]
}
